pipeline {
	agent any
	
	stages {
		stage ('Compile Stage') {
			steps {
				withMaven (maven: 'MVN_3.5.4')
				powershell 'mvn clean install'
				echo 'Hello World'
				}
			}
		stage ('Test Stage') {
			steps {
				withMaven (maven: 'MVN_3.5.4')
				powershell 'mvn clean test'
				}
			}
		stage ('Deploy Stage') {
			steps {
				withMaven (maven: 'MVN_3.5.4')
				powershell 'mvn deploy'
				}
			}
		}
	}